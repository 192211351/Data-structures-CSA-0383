#include<stdio.h>
int main()
{
	int a[10][10],b[10][10],mul[10][10],i,j,c,r;
	printf("enter the number of rows:");
	scanf("%d",&r);
	printf("enter the number of coloumns:");
	scanf("%d",&c);
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&b[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
		mul[i][j]=a[i][j]*b[i][j];
	}
}
	
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			printf("%d \t",mul[i][j]);
		}printf("\n");
	}
	return 0;
	
}
